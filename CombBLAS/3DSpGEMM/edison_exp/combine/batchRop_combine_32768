cd $PBS_O_WORKDIR
export MPICH_GNI_COLL_OPT_OFF=MPI_Alltoallv
IN1=/scratch2/scratchdirs/azad/spGEMM_matrices/cage15.mtx
IN2=/scratch2/scratchdirs/azad/spGEMM_matrices/delaunay_n24.mtx
