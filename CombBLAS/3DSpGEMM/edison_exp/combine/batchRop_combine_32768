IN7=/scratch2/scratchdirs/azad/spGEMM_matrices/it-2004.mtx

export OMP_NUM_THREADS=6

aprun -n 5408 -d 6 -N 4 -S 2 -cc depth ../../RestrictionOp 26 26 8 input $IN1
